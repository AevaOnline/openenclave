enclave {
    struct addition_args {
        int arg1;
        int arg2;
        int sum;
    };

    trusted {
          public int standard_enc_sum(
              int arg1,
              int arg2);
          public int synchronous_switchless_enc_sum(
              int arg1,
              int arg2) transition_using_threads;
          public void batch_enc_sum(
              [in, out, count=count] addition_args* args,
              size_t count);
    };
};
